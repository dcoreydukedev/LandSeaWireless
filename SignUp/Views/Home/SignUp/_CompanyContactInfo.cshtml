<div class="wizard_step_content_partial_view_wrapper" id="contact_info_company">

    <div class="form-group row">
        <label for="txtCompanyName" class="col-sm-2 col-form-label">Company Name</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="txtCompanyName" />
        </div>
    </div>

    <div class="container" id="contact_info_company_contact_info">

        <h3>Contact Info:</h3>

        <div class="form-group row">
            <label for="txtFirstName" class="col-sm-2 col-form-label">First Name</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="txtFirstName" />
            </div>
        </div>
        <div class="form-group row">
            <label for="txtLastName" class="col-sm-2 col-form-label">Last Name</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="txtLastName" />
