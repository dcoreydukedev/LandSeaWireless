{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [ "ES6" ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noResolve": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES6",
    "types": [
      "react",
      "react-dom"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "modules",
    "components"
  ],
  "files": [ "wizard.tsx" ]
}
